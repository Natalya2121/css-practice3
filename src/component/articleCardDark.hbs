{{#with data}}
    <div class="card">
        <div class="card_image" style="background-image: url({{photo}})">{{#if isNew}}<span class="card_new">NEW</span>{{/if}}</div>
        <h2 class="card_title">{{name}}</h2>
        <p class="card_text">{{description}}</p>
        <a href="#" class="card_button">Read All</a>
    </div>
{{/with}}